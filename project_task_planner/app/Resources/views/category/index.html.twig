{% extends 'base.html.twig' %}

{% block body %}
    <h1>Category list</h1>
    <tr>
        <td>
            {% for category in categories %}
                {{ category.name }}
            {% endfor %}
        </td>
        <td>
            <ul>
                <li><a href="{{ path('app_category_edit', { 'id': category.id }) }}">edit</a></li>
            </ul>
        </td>
    </tr>

{% endblock %}